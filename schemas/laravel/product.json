{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Product Schema",
    "required": [
        "checked",
        "dimensions",
        "id",
        "name",
        "price"
    ],
    "properties": {
        "checked": {
            "type": "boolean",
            "title": "The Checked Schema",
            "default": false,
            "examples": [
                false
            ]
        },
        "dimensions": {
            "type": "object",
            "title": "The Dimensions Schema",
            "required": [
                "width"
            ],
            "properties": {
                "width": {
                    "type": "integer",
                    "title": "The Width Schema",
                    "default": 0,
                    "examples": [
                        5
                    ]
                }
            }
        },
        "id": {
            "type": "integer",
            "title": "The Id Schema",
            "default": 0,
            "examples": [
                1
            ]
        },
        "name": {
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
                "A green door"
            ],
            "pattern": "^(.*)$"
        },
        "price": {
            "type": "number",
            "title": "The Price Schema",
            "default": 0.0,
            "examples": [
                12.5
            ]
        }
    }
}
